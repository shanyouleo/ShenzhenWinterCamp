    "def bifunc(f,a,b):\n",
    "    if abs(f(a))<=1e-10:\n",
    "        return a\n",
    "    if abs(f(b))<=1e-10:\n",
    "        return b\n",
    "    if f((a+b)/2)*f(b)<=0:\n",
    "        return bifunc(f,(a+b)/2,b)\n",
    "    if  f((a+b)/2)*f(a)<=0:\n",
    "        return bifunc(f,a,(a+b)/2)\n",
